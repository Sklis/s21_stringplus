#test strtok_1
char str1[] = "- This,a,sample,string.";
char str2[] = "- This,a,sample,string.";
char delim[] = ",";
char *lek1;
char *lek2;
lek1 = strtok(str1, delim);
lek2 = s21_strtok(str2, delim);
while (lek1 != s21_NULL) {
  ck_assert_pstr_eq(lek1, lek2);
  lek1 = strtok(s21_NULL, delim);
  lek2 = s21_strtok(s21_NULL, delim);
}

